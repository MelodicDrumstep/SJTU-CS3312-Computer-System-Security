        "gift-card/settle-result/index",
        "gift-card/order-list/index",
        "gift-card/order-detail/index",
        "gift-card/card-detail/index",
        "gift-card/record-list/index",
        "gift-card/multi-send/index",
        "gift-card/pick-up/index",
        "gift-card/select/index"
      ]
    },
    {
      "root": "pages/duiba/",
      "pages": [
        "duibaPay/duibaPay",
        "duibaRedirect/duibaRedirect"
      ]
    }
  ],
  "plugins": {}
}