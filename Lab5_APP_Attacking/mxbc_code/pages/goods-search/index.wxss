page {
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    background-color: #fff;
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
    height: 100vh;
    overflow: hidden;
    padding: 24rpx 24rpx 0
}

.search-form {
    margin-bottom: 40rpx;
    position: relative
}

.icon-search {
    left: 24rpx;
    position: absolute;
    z-index: 9
}

.search-input {
    background: #f1f1f2;
    border-radius: 36rpx;
    box-sizing: border-box;
    height: 72rpx;
    padding: 0 80rpx 0 76rpx;
    width: 620rpx
}

.content {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    position: relative
}

.icon-clear {
    height: 100%;
    position: absolute;
    right: 80rpx;
    top: 0;
    width: 80rpx;
    z-index: 9
}

.history {
    max-height: 244rpx;
    overflow: hidden
}

.history-item,.suggest-item {
    background: hsla(0,0%,56%,.1);
    border-radius: 12rpx;
    display: inline-block;
    line-height: 70rpx;
    margin: 0 24rpx 24rpx 0;
    max-width: 390rpx;
    padding: 0 24rpx
}

.suggest-title {
    margin: 24rpx 0;
    position: relative;
    z-index: 2
}

.suggest-title::after {
    background: url(data:image/png;base64,) 50%/100% no-repeat;
    bottom: -4rpx;
    content: "";
    display: inline-block;
    height: 38rpx;
    left: -22rpx;
    position: relative;
    width: 40rpx;
    z-index: -1
}

.suggest-item.strong {
    background: #fff8f8;
    color: #fc3f41
}

.suggest-item.strong::after {
    background: url(data:image/png;base64,) 50%/100% no-repeat;
    content: "";
    display: inline-block;
    height: 36rpx;
    margin-bottom: 6rpx;
    vertical-align: middle;
    width: 36rpx
}

.no-goods,.result-list {
    -webkit-overflow-scrolling: touch;
    background: #fff;
    height: 100%;
    overflow: hidden scroll;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 9
}

.goods-item:last-child {
    padding-bottom: 50rpx
}

.no-goods {
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
    padding-top: 190rpx
}
