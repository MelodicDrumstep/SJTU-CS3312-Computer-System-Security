page {
    background-color: #f5f5f5;
    overflow: hidden
}

.another-order {
    border: 1rpx solid var(--theme-color);
    border-radius: 8rpx;
    display: inline-block;
    height: 56rpx;
    line-height: 56rpx;
    text-align: center;
    width: 128rpx
}

.top-wrap {
    background-color: #fff;
    border-bottom: 2rpx solid #f5f5f5
}

.list-container {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: relative;
    -webkit-transition: left .3s;
    transition: left .3s;
    width: 300%
}

.scroll-list {
    height: calc(100vh - 88rpx);
    position: relative;
    width: 100vw
}

.cells {
    background: #f5f5f5;
    padding-top: 24rpx
}

.cell {
    -webkit-box-flex: 1;
    background: #fff;
    border-radius: var(--card-radius);
    -webkit-flex: 1;
    flex: 1;
    margin: 0 24rpx 24rpx;
    padding: 24rpx
}

.cell .title {
    -webkit-box-pack: left;
    background-color: teal;
    -webkit-justify-content: left;
    justify-content: left
}

.cell .title,.shop {
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex
}

.shop {
    max-width: 530rpx
}

.out {
    background: var(--bg-theme-light-color);
    border-radius: 8rpx;
    color: #ff5254;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    font-size: 20rpx;
    font-weight: 600;
    line-height: 28rpx;
    margin-right: 16rpx;
    padding: 4rpx 8rpx
}

.shopName {
    font-size: 28rpx;
    font-weight: 500;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap
}

.titleTag {
    -webkit-box-pack: center;
    -webkit-box-align: center;
    -webkit-align-content: center;
    align-content: center;
    -webkit-align-items: center;
    align-items: center;
    background-color: #ff9f00;
    border-radius: 4rpx;
    color: #fff;
    font-size: 20rpx;
    font-weight: 500;
    height: 42rpx;
    -webkit-justify-content: center;
    justify-content: center;
    line-height: 42rpx;
    margin-left: 4rpx;
    text-align: center;
    vertical-align: center;
    width: 88rpx
}

.order-status {
    color: #aaa;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    font-size: 28rpx;
    margin-left: 50rpx
}

.icon-arrow_right {
    margin-left: 8rpx
}

.products {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    margin-top: 24rpx;
    width: 100%
}

.productImg {
    border-radius: 12rpx;
    height: 116rpx;
    margin-right: 16rpx;
    width: 116rpx
}

.productName {
    -webkit-align-content: center;
    align-content: center;
    color: #777;
    font-size: 24rpx;
    font-weight: 400;
    line-height: 34px;
    margin-top: 8rpx;
    max-width: 150rpx;
    overflow: hidden;
    text-overflow: ellipsis
}

.priceBox {
    color: #333;
    margin-left: 6rpx;
    width: 116rpx
}

.psum {
    -webkit-box-pack: end;
    -webkit-justify-content: flex-end;
    justify-content: flex-end;
    margin-top: 28rpx
}

.priceCount,.psum {
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex
}

.priceCount {
    -webkit-box-flex: 0.5;
    -webkit-flex: 0.5;
    flex: 0.5;
    width: 46%
}

.empty-view {
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
    flex-direction: column;
    height: 80%
}

.empty-text {
    color: #999;
    font-size: 24rpx;
    font-weight: 400
}

.empty-btn {
    margin-top: 42rpx;
    width: 240rpx
}

.operate-box {
    -webkit-box-align: center;
    -webkit-box-pack: end;
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: flex-end;
    justify-content: flex-end
}

.birthday-img {
    border-radius: 8rpx;
    box-shadow: 6rpx 8rpx 8rpx hsla(0,100%,89%,.81);
    height: 56rpx;
    width: 218rpx
}

.dialog-cancel {
    padding: 22rpx 0;
    text-align: center;
    width: 100%
}
