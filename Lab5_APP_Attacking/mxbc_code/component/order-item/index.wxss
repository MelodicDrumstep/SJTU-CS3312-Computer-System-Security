.shop {
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    max-width: 530rpx
}

.out {
    background: var(--bg-theme-light-color);
    border-radius: 8rpx;
    color: #ff5254;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    font-size: 20rpx;
    font-weight: 600;
    line-height: 28rpx;
    margin-right: 16rpx;
    padding: 4rpx 8rpx
}

.shopName {
    font-size: 28rpx;
    font-weight: 500;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap
}

.titleTag {
    -webkit-box-pack: center;
    -webkit-box-align: center;
    -webkit-align-content: center;
    align-content: center;
    -webkit-align-items: center;
    align-items: center;
    background-color: #ff9f00;
    border-radius: 4rpx;
    color: #fff;
    font-size: 20rpx;
    font-weight: 500;
    height: 42rpx;
    -webkit-justify-content: center;
    justify-content: center;
    line-height: 42rpx;
    margin-left: 4rpx;
    text-align: center;
    vertical-align: center;
    width: 88rpx
}

.order-status {
    color: #aaa;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    font-size: 28rpx;
    margin-left: 50rpx
}

.icon-arrow_right {
    margin-left: 8rpx
}

.products {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    margin-top: 24rpx;
    width: 100%
}

.productImg {
    border-radius: 12rpx;
    height: 116rpx;
    margin-right: 16rpx;
    width: 116rpx
}

.productName {
    -webkit-align-content: center;
    align-content: center;
    color: #777;
    font-size: 24rpx;
    font-weight: 400;
    line-height: 34px;
    margin-top: 8rpx;
    max-width: 150rpx;
    overflow: hidden;
    text-overflow: ellipsis
}

.priceBox {
    color: #333;
    font-size: 24rpx;
    margin-left: 6rpx;
    text-align: right;
    width: 60rpx
}

.psum {
    -webkit-box-pack: end;
    -webkit-justify-content: flex-end;
    justify-content: flex-end;
    margin-top: 28rpx
}

.priceCount,.psum {
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex
}

.priceCount {
    -webkit-box-flex: 0.5;
    -webkit-flex: 0.5;
    flex: 0.5;
    width: 46%
}

.empty-view {
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
    flex-direction: column;
    height: 80%
}

.empty-text {
    color: #999;
    font-size: 24rpx;
    font-weight: 400
}

.empty-btn {
    margin-top: 42rpx;
    width: 240rpx
}

.order-takeno {
    background-color: #fff8f8;
    border-radius: 16rpx;
    font-size: 32rpx;
    padding: 16rpx 24rpx;
    white-space: wrap
}

.order-takeno_text {
    height: 40rpx;
    margin-right: 8rpx;
    width: 96rpx
}

.menu_cell-shop_name {
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    border: 2rpx solid #fff;
    border-bottom: none;
    border-radius: 24rpx 24rpx 0 0;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    height: 72rpx;
    padding: 0 20rpx
}

.menu_cell-page {
    padding: 24rpx
}

.menu_cell-body_products_detail {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    width: 380rpx
}

.mzhd-bar {
    height: 36rpx
}

.bar-bg {
    background: #ffc9c9;
    height: 12rpx;
    margin-right: 16rpx;
    overflow: hidden;
    width: 120rpx
}

.bar,.bar-bg {
    border-radius: 6rpx
}

.bar {
    background: #fc3f41;
    height: 100%
}
