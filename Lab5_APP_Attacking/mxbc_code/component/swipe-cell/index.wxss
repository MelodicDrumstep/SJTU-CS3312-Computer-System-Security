.swipe-cell {
    overflow: hidden;
    position: relative
}

.swipe-cell-wrap {
    position: relative;
    -webkit-transition: -webkit-transform .6s cubic-bezier(.18,.89,.32,1);
    transition: -webkit-transform .6s cubic-bezier(.18,.89,.32,1);
    transition: transform .6s cubic-bezier(.18,.89,.32,1);
    transition: transform .6s cubic-bezier(.18,.89,.32,1),-webkit-transform .6s cubic-bezier(.18,.89,.32,1)
}

.swipe-cell__right {
    height: 100%;
    position: absolute;
    right: 0;
    top: 0;
    -webkit-transform: translate3d(100%,0,0);
    transform: translate3d(100%,0,0)
}
