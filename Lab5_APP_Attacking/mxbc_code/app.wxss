page {
    --bg-theme-color: #e60012;
    --theme-color: #e60012;
    --disable-theme-color: #f8b3b8;
    --bg-theme-light-color: #fff8f8;
    --theme-light-color: #fc3f41;
    --text-theme-color: #ff5254;
    --warn-color: #ff9f00;
    --danger-color: #fc3f41;
    --button-radius: 16rpx;
    --card-radius: 24rpx;
    --tag-radius: 8rpx;
    --theme-padding: 24rpx;
    box-sizing: border-box;
    color: #333;
    font-family: -apple-system,Helvetica Neue,Helvetica,Segoe UI,Arial,Roboto,PingFang SC,miui,Hiragino Sans GB,Microsoft Yahei,sans-serif;
    font-size: 32rpx;
    position: relative
}

::-webkit-scrollbar {
    color: transparent;
    display: none;
    height: 0;
    width: 0
}

wx-button {
    -webkit-box-align: center;
    -webkit-box-pack: center;
    -webkit-align-items: center;
    align-items: center;
    background-color: #e60012;
    border: none;
    border-radius: 16rpx;
    color: #fff;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    font-size: 32rpx;
    font-weight: 500;
    height: 88rpx;
    -webkit-justify-content: center;
    justify-content: center;
    margin: 0;
    overflow: inherit;
    padding: 0 28rpx
}

wx-button::after {
    border: 1px solid transparent;
    border-radius: 32rpx;
    box-sizing: border-box;
    content: "";
    height: 200%;
    left: 0;
    position: absolute;
    top: 0;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    width: 200%
}

wx-button.plain {
    background-color: #fff;
    color: #e60012
}

wx-button.plain::after {
    border-color: #e60012
}

wx-button.default {
    background-color: #fff;
    color: #333
}

wx-button.default::after {
    border-color: #aaa
}

wx-button[disabled]:not([type]) {
    background-color: #f8b3b8;
    color: #fff
}

wx-button.small {
    border-radius: 8rpx;
    display: inline-block;
    font-size: 24rpx;
    height: 48rpx;
    line-height: 48rpx;
    padding: 0 16rpx
}

wx-button.mini {
    display: inline-block;
    font-size: 22rpx;
    height: 50rpx;
    line-height: 50rpx
}

.button-hover {
    background-color: #d20010;
    color: #fff
}

.plain.button-hover {
    background-color: #fff8f8;
    color: #e60012
}

.default.button-hover {
    background-color: #eee;
    color: #333
}

.wx-swiper-dot {
    border-radius: 8rpx!important;
    height: 8rpx!important;
    margin: 0 4rpx!important;
    width: 8rpx!important
}

.wx-swiper-dot-active {
    width: 30rpx!important
}

.avatar-bg {
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 100% 100%
}

.birthday-hat::after {
    background: url(data:image/png;base64,) 50%/100% no-repeat;
    content: "";
    height: 40rpx;
    left: 22rpx;
    position: absolute;
    top: 14rpx;
    width: 43rpx
}

